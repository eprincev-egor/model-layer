{
  "name": "model-layer",
  "version": "2.2.10",
  "description": "model layer",
  "main": "dist/index.js",
  "types": "lib/index.ts",
  "directories": {
    "dist": "dist",
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json 'lib/**/*.ts' && tslint -c test/tslint.json 'test/**/*.ts'",
    "test": "nyc mocha",
    "prepublishOnly": "tsc"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "exclude": [
      "dist",
      "coverage"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eprincev-egor/model-layer.git"
  },
  "keywords": [
    "model",
    "data",
    "layer",
    "tree",
    "data",
    "structure",
